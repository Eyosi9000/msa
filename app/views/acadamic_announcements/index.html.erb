<p style="color: green"><%= notice %></p>

<h1>Acadamic announcements</h1>


<table class="table table-striped table-hover table-bordered">
  <thead>
    <tr>
      <th>Start Date</th>
      <th>End Date</th>
      <th>Status</th>
      <th>Acadamic Program</th>
      <th>Acadamic Year</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @acadamic_announcements.each do |acadamic_announcement| %>
      <tr>
        <td><%= acadamic_announcement.start_date %></td>
        <td><%= acadamic_announcement.end_date %></td> 
        <td><%= acadamic_announcement.status %></td>
        <td><%= acadamic_announcement.acadamic_program_id %></td>
        <td><%= acadamic_announcement.acadamic_year_id %></td>
        <td>
          <%= link_to "Show", acadamic_announcement %>
          &nbsp;
          <%= link_to "Edit", edit_acadamic_program_path(acadamic_announcement) %>
        </td>
      </tr>
    <% end %>
  </tbody>

</table>

<%= link_to "New acadamic announcement", new_acadamic_announcement_path %>
